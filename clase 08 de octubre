CREATE TABLE PRUEBA_EXCEPCIONES(ID_PRUEBA INTEGER, NOMBRE VARCHAR2(120));

BEGIN 
FOR I IN 1..10 LOOP
INSERT INTO PRUEBA_EXCEPCIONES VALUES(I,'JUAN');
END LOOP;
END;
/

SELECT * FROM PRUEBA_EXCEPCIONES;


*/ ESTE BLOQUE NO MUESTRA UNA EXCEPCION. lLEVA UN WHERE/*

DECLARE 
MI_USUARIO PRUEBA_EXCEPCIONES.NOMBRE%TYPE;
BEGIN
SELECT NOMBRE INTO MI_USUARIO FROM PRUEBA_EXCEPCIONES WHERE ID_PRUEBA=3;
DBMS_OUTPUT.PUT_LINE('USUARIO ENCONTRADO PARA PODER OPERARLO: ' ||MI_USUARIO);
END;
/

*/ ESTE BLOQUE ESTA CON LA EXCEPCION TOO_MANY_ROWS, ESTA DEFINIDA POR EL USUARIO/*

DECLARE 
MI_USUARIO PRUEBA_EXCEPCIONES.NOMBRE%TYPE;
BEGIN
SELECT NOMBRE INTO MI_USUARIO FROM PRUEBA_EXCEPCIONES;
DBMS_OUTPUT.PUT_LINE('USUARIO ENCONTRADO PARA PODER OPERARLO: ' ||MI_USUARIO);
EXCEPTION
WHEN TOO_MANY_ROWS THEN
DBMS_OUTPUT.PUT_LINE('SE LANZO UNA EXCEPCION PORQUE HUBO MUCHAS COINCIDENCIAS');
END;
/


*/ESTA EXEPCION SE MUESTRA CON UN BLOQUE PLQ SIN UN PROCEDIMIENTO ALMECANDO
RAISE_APPLICATION_ERROR.......NECESITA DOS ARGUMENTOS /*

DECLARE
EDAD INTEGER;
BEGIN
EDAD:=19;
IF EDAD < 18 THEN
RAISE_APPLICATION_ERROR(-20001,'LO SIENTO ERES MUY PEQUEÃ‘O');
END IF;
END;
/





