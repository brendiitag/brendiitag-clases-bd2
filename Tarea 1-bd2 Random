CREATE TABLE REGISTRO(ID INTERGER,EDAD INTERGER,NOMBRE VARCHAR(50));
BEGIN
FOR I IN 1..300
DMBS_OUTPUT.PUT_LINE(I||'SEGUNDO CICLO');
INSERT INTO REGISTRO RANDOM.VALUES(I,1..40,'JUAN'||'Pedro'||'Ana');
END LOOP;
END;
SELECT*FROM REGISTRO ORDER BY DMBS_RANDOM.VALUE WHERE ROWNUM=1;

/
